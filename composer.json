{
    "name": "wapilot/sdk",
    "description": "Official PHP/Laravel SDK for WAPILOT.io - WhatsApp Business API Integration",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Wapilot\\SDK\\": "src/"
        }
    },
    "authors": [
        {
            "name": "WAPILOT",
            "email": "support@wapilot.io"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wapilot\\SDK\\WapilotServiceProvider"
            ],
            "aliases": {
                "Wapilot": "Wapilot\\SDK\\Facades\\Wapilot"
            }
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/wapilot/wapilot-php-sdk"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "github-oauth": {
            "github.com": "YOUR_GITHUB_TOKEN"
        }
    }
}